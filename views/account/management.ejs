<% if (title) { %>
    <h1 class="login-Title"><%= title %></h1>
    <% } else {
      res.redirect('/')
} %>

<% if (messages) { %>
    <%- messages() %>
<%}%> 

<% if (errors) { %>
    <ul class="notice">
    <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
    <%  }) %>
    </ul>
<% } %>

<h2>You're logged in</h2>

<%- greet %>